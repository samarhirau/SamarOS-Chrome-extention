{
     "manifest_version": 3,
     "name": "SamarOS New Tab",
     "version": "1.0",
     "description": "A customizable new tab page inspired by SamarOS design.",
     "chrome_url_overrides": {
       "newtab": "newtab.html"
     },
     "icons": {
       "16": "icons/icon16.png",
       "48": "icons/icon48.png",
       "128": "icons/icon128.png"
     },
     "permissions": ["storage"],
     "background": {
       "service_worker": "background.js"
     }
   }